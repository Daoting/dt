{
  // 默认数据源键名
  "DbKey": "dt",
  // 服务名称
  "SvcName": "cosm",

  // 是否缓存sql字典
  "CacheSql": false,
  // 是否输出所有运行中的Sql语句
  "TraceSql": true,
  // 是否输出所有调用的Api名称
  "TraceRpc": true,

  // 启动KestrelServer时的监听设置
  "KestrelListen": [
    {
      "Scheme": "https",
      "Address": "0.0.0.0",
      "Port": "20204"
    }
  ],

  // 是否为单体服务，true时合并所有微服务为一个服务，微服务包括：
  // 1. cm msg fsm三个标准微服务
  // 2. 当前服务
  // 3. CustomSvcDbKey配置的服务
  "IsSingletonSvc": true,

  /********** 以下配置只在单体时有效 **********/

  // 自定义服务的数据源键名
  // 1. 可以自定义cm msg fsm服务或当前服务的数据源键名，否则采用默认数据源键名
  // 2. 定义任意其它服务的数据源键名，实现跨多个数据源操作
  "CustomSvcDbKey": {
    //"svcname": "dbkey"
  },

  // 提供给fsm服务
  "FixedVolume": "photo;editor;g;chat",
  "MaxRequestBodySize": 1073741824
}