{
  //"Mode": "InitDb",

  "DbKey": "mydt",
  "TraceSql": true,
  "TraceRpc": true,

  // 启动KestrelServer时的监听设置
  "KestrelListen": [
    {
      "Scheme": "http",
      "Address": "0.0.0.0",
      "Port": "30001"
    },
    {
      "Scheme": "https",
      "Address": "0.0.0.0",
      "Port": "20201"
    }
  ],

  // 每个服务的url
  "SvcUrls": {
    "msg": "*/dt-msg",
    "fsm": "*/dt-fsm",
    "demo": "*/dt-demo"
  },

  // 除默认数据源外，其余需要导出到模型库的数据源键名
  "ExportToModel": []
}