<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="bin\Release\net5.0\Dt.Fsm.exe" arguments="" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
      <security>
        <requestFiltering>
          <!--上传限制1G，和service.json的MaxRequestBodySize相同-->
          <requestLimits maxAllowedContentLength="1024000000" />
        </requestFiltering>
      </security>
    </system.webServer>
  </location>
</configuration>