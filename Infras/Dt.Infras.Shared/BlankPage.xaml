<Page
    x:Class="Dt.Shell.BlankPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Padding="10" Spacing="20">
            <Button Content="返回" Click="OnBack" />
            <Button Content="切换行头显示" Click="OnToggleRowHearder" />
            <Button Content="切换列头显示" Click="OnToggleColHearder" />
            <Button Content="切换网格显示" Click="OnToggleGridLine" />
        </StackPanel>
        <a:Excel x:Name="_excel" Grid.Row="1" />
    </Grid>
</Page>
