<a:Win
    x:Class="Dt.Sample.SamplesMain"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    AutoSaveLayout="False"
    Title="样例">

    <a:Pane>
        <a:Tabs>
            <a:Tab Order="1" Icon="词典">
                <a:NavList x:Name="_navControl" Title="控件" To="NewWin" ViewMode="Tile" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Pane Pos="Right">
        <a:Tabs>
            <a:Tab Order="2" Icon="搬运工">
                <a:NavList x:Name="_navModule" Title="模块" To="NewWin" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
