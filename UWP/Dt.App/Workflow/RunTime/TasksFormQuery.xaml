<a:Win
    x:Class="Dt.App.Workflow.TasksFormQuery"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.App.Workflow">

    <a:Pane>
        <a:Tabs>
            <a:Tab Title="列表">
                <a:Lv x:Name="_lvTask" ItemClick="OnTaskItemClick">
                    <DataTemplate>
                        <a:Dot ID="name" Margin="10" />
                    </DataTemplate>
                </a:Lv>
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
