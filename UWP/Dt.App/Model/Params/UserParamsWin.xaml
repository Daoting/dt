<a:Win
    x:Class="Dt.App.Model.UserParamsWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.App.Model">

    <a:Pane>
        <a:Tabs>
            <a:Tab>
                <a:SearchMv x:Name="_search" Placeholder="参数名" Search="OnSearch">
                    <x:String>全部</x:String>
                    <x:String>最近修改</x:String>
                </a:SearchMv>
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab Order="1">
                <l:UserParamsList x:Name="_list" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
    <a:Pane Pos="Right">
        <a:Tabs>
            <a:Tab>
                <l:UserParamsForm x:Name="_form" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
