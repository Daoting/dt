<a:Win
    x:Class="Dt.App.Model.PrvWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.App.Model">

    <a:Pane>
        <a:Tabs>
            <a:Tab Order="1">
                <l:PrvList x:Name="_list" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab>
                <l:PrvForm x:Name="_form" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
    <a:Pane Pos="Right">
        <a:Tabs>
            <a:Tab>
                <l:PrvRoleList x:Name="_roleList" />
            </a:Tab>
            <a:Tab>
                <l:PrvUserList x:Name="_userList" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
