        <a:CText ID="param3" Title="参数3" />
        <a:CList ID="unuse" Title="辅助" SrcID="id" TgtID="idstr">
            <a:CList.Items>
                <a:IDStr ID="0" Str="选项一" />
                <a:IDStr ID="1" Str="选项二" />
                <a:IDStr ID="2" Str="选项三" />
            </a:CList.Items>
