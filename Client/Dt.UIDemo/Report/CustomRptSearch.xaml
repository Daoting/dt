    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

    <a:Tab.Menu>
        <a:Menu>
            <a:Mi ID="查询" Icon="搜索" ShowInPhone="Icon" Click="OnQuery" />
