<a:Win
    x:Class="Dt.Base.Tools.RefreshSqliteWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="更新缓存文件">

    <a:Main>
        <a:Tabs>
            <a:Tab Title="缓存文件列表">
                <a:Lv x:Name="_lvFile" >
                    <a:Ex.Menu>
                        <a:Menu>
                            <a:Mi ID="更新" Icon="刷新" Click="OnRefreshFile" />
                        </a:Menu>
                    </a:Ex.Menu>
                    <DataTemplate>
                        <a:Dot Margin="10,0,10,0" />
                    </DataTemplate>
                </a:Lv>
            </a:Tab>
        </a:Tabs>
    </a:Main>
    
</a:Win>
