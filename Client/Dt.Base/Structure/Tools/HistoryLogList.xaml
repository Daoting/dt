<a:Tab
    x:Class="Dt.Base.Tools.HistoryLogList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="日志列表">

    <a:Lv x:Name="_lv" ItemClick="OnOutputClick" ItemHeight="NaN">
        <DataTemplate>
            <StackPanel Padding="10,0,10,10">
                <StackPanel Padding="10,0,10,10">
                    <StackPanel Height="40" Orientation="Horizontal">
                        <a:Dot ID="Time" Foreground="{StaticResource 深灰1}" />
                        <a:Dot ID="LevelAndSource" Call="HisLogCellUI.TitleBrush" Margin="10, 6, 0, 6" Padding="10,0,10,0" />
                    </StackPanel>
                    <a:Dot ID="Message" />
                </StackPanel>
            </StackPanel>
        </DataTemplate>
    </a:Lv>
</a:Tab>