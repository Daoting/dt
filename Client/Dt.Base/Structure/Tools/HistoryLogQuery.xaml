<a:Tab
    x:Class="Dt.Base.Tools.HistoryLogQuery"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="查询"
    Icon="漏斗">
    
    <a:QueryFv x:Name="_fv">
        <a:CBool ID="eninclude" Title="包含，文字之间用 &amp; 和 | 表示与或关系" ShowTitle="False" />
        <a:CText ID="include" ShowTitle="False" />
        <a:CBool ID="enexclude" Title="排除，文字之间用 &amp; 和 | 表示与或关系" ShowTitle="False" />
        <a:CText ID="exclude" ShowTitle="False" />
        <a:CBool ID="enline" Title="行号范围" ShowTitle="False" />
        <a:CNum ID="startno" IsInteger="True" Title="始" TitleWidth="40" ColSpan="0.5" />
        <a:CNum ID="endno" IsInteger="True" Title="终" TitleWidth="40" ColSpan="0.5" />
        <a:CList ID="level" Title="级别">
            <a:CList.Items>
                <x:String>全部</x:String>
                <x:String>Debug</x:String>
                <x:String>Information</x:String>
                <x:String>Warning</x:String>
                <x:String>Error</x:String>
                <x:String>Fatal</x:String>
            </a:CList.Items>
        </a:CList>
    </a:QueryFv>
</a:Tab>