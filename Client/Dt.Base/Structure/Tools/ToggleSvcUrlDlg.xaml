<a:Dlg
    x:Class="Dt.Base.Tools.ToggleSvcUrlDlg"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

    <a:Dlg.Menu>
        <a:Menu>
            <a:Mi ID="切换服务" Icon="服务器" />
        </a:Menu>
    </a:Dlg.Menu>

    <a:Fv x:Name="_fv">
        <a:CList ID="url" Title="服务Url" IsEditable="true">
            <a:CList.Items>
                <x:String>选项一</x:String>
                <x:String>选项二</x:String>
                <x:String>选项三</x:String>
                <x:String>选项四</x:String>
                <x:String>选项五</x:String>
                <x:String>选项六</x:String>
            </a:CList.Items>
        </a:CList>
        <a:CBar Title="以后始终使用该服务" />
        <a:CBool ID="always" Title="以后始终使用该服务" ShowTitle="false" />
    </a:Fv>
</a:Dlg>
