<UserControl
    x:Class="Dt.Base.NotifyItem"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid x:Name="_grid" MinHeight="50">
        <Rectangle x:Name="_rc" IsHitTestVisible="False" />
        <StackPanel x:Name="_sp" Margin="20,10" VerticalAlignment="Center">
            <TextBlock x:Name="_tb" Foreground="White" TextWrapping="Wrap" />
        </StackPanel>
    </Grid>
</UserControl>
