<a:Win
    x:Class="Dt.Base.Report.RptDesignWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Home="编辑,报表项,单元格">

    <a:WinCenter>
        <a:Tabs>
            <a:Tab Title="编辑">
                <a:Tab.Menu>
                    <a:Menu x:Name="_menu">
                        <a:Mi ID="撤消" Icon="撤销" />
                        <a:Mi ID="重做" Icon="重做" />
                        <a:Mi ID="网格" IsCheckable="True" Click="OnCheckChanged" />
                    </a:Menu>
                </a:Tab.Menu>
                <a:Excel x:Name="_excel"
                         TabStripInsertTab="False"
                         TabStripEditable="False"
                         ShowDecoration="True"
                         CanUserDragDrop="False" />
            </a:Tab>
        </a:Tabs>
    </a:WinCenter>
    <a:WinItem DockState="DockedRight">
        <a:Tabs>
            <a:Tab x:Name="_tabItem" Title="报表项" />
            <a:Tab x:Name="_tabCell" Title="单元格" />
        </a:Tabs>
    </a:WinItem>
</a:Win>
