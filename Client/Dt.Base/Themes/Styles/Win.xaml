<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:docking="using:Dt.Base.Docking">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///Dt.Base/Themes/Global.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!-- Win -->
    <Style TargetType="a:Win">
        <Setter Property="Foreground" Value="{StaticResource 默认前景}" />
        <Setter Property="Background" Value="{StaticResource 默认背景}" />
        <Setter Property="BorderBrush" Value="{StaticResource 浅灰2}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:Win">
                    <Grid x:Name="RootGrid">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <!-- 内容区域面板 -->
                        <docking:PanePanel x:Name="RootPanel" Grid.Column="1" Grid.Row="1" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Pane -->
    <Style TargetType="a:Pane">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:Pane">
                    <Grid>
                        <docking:TabItemPanel
                            x:Name="TabItemPanel"
                            Orientation="{TemplateBinding Orientation}" />
                        <docking:GridResizer
                            x:Name="Resizer"
                            Placement="{TemplateBinding ResizerPlacement}" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Tabs -->
    <Style TargetType="a:Tabs">
        <Setter Property="BorderBrush" Value="{StaticResource 浅灰2}" />
        <Setter Property="Background" Value="{StaticResource 默认背景}" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="VerticalContentAlignment" Value="Stretch" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:Tabs">
                    <Grid Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>

                        <!-- Header -->
                        <docking:TabHeader
                            x:Name="HeaderElement"
                            Visibility="{Binding ShowHeader, Converter={StaticResource CvBoolToVisible}, RelativeSource={RelativeSource TemplatedParent}}" />

                        <!-- TabControl -->
                        <Grid x:Name="MainGrid" Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <!-- 用在win模式的Tabs、phone模式的Tab、Dlg，为Lv Fv等内容提供区域大小 -->
                            <a:SizedPresenter
                                x:Name="TabContent"
                                Content="{TemplateBinding SelectedContent}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="1"
                                ContentTransitions="{TemplateBinding ContentTransitions}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                Margin="0,0,-1,0"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                Grid.ColumnSpan="2" />
                            <!-- 标签 -->
                            <StackPanel
                                x:Name="ItemsPanel"
                                Orientation="Horizontal"
                                Grid.ColumnSpan="2"
                                Grid.Row="1">
                                <StackPanel.ChildrenTransitions>
                                    <TransitionCollection>
                                        <ReorderThemeTransition />
                                    </TransitionCollection>
                                </StackPanel.ChildrenTransitions>
                            </StackPanel>
                        </Grid>

                        <docking:GridResizer
                            x:Name="Resizer"
                            Placement="{TemplateBinding ResizerPlacement}"
                            Grid.RowSpan="2" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Tabs PhoneUI模式始终不在可视树 -->
    <Style x:Key="PhoneEmptyTabs" TargetType="a:Tabs">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:Tabs">
                    <Grid />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Tab windows -->
    <Style x:Key="WinTab" TargetType="a:Tab">
        <Setter Property="Foreground" Value="{StaticResource 默认前景}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BackButtonVisibility" Value="Collapsed" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:Tab">
                    <Grid
                        x:Name="ElementGrid"
                        Background="{TemplateBinding Background}">
                        <a:RotateContent
                            x:Name="ElementHeader"
                            HorizontalContentAlignment="Stretch"
                            Rotate="Horizontal"
                            VerticalContentAlignment="Stretch">
                            <Grid x:Name="Container" Height="{StaticResource RowInnerHeight}">
                                <Rectangle
                                    x:Name="NormalBorder"
                                    Fill="{StaticResource 浅灰1}"
                                    IsHitTestVisible="False"
                                    Margin="0,-1,-1,0"
                                    Stroke="{TemplateBinding BorderBrush}"
                                    StrokeThickness="1" />
                                <Rectangle
                                    x:Name="SelectedBorder"
                                    Fill="White"
                                    IsHitTestVisible="False"
                                    Margin="1,-1,0,1"
                                    Visibility="Collapsed" />
                                <Rectangle
                                    x:Name="ElementLine"
                                    Height="2"
                                    IsHitTestVisible="False"
                                    VerticalAlignment="Bottom" />
                                <TextBlock
                                    x:Name="HeaderText"
                                    Text="{TemplateBinding Title}"
                                    HorizontalAlignment="Center"
                                    Margin="15,0,15,0"
                                    VerticalAlignment="Center" />
                            </Grid>
                        </a:RotateContent>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="SelectionStates">
                                <VisualState x:Name="UnSelected" />
                                <VisualState x:Name="PointerOver">
                                    <VisualState.Setters>
                                        <Setter Target="NormalBorder.Fill" Value="{StaticResource 浅黄}" />
                                        <Setter Target="ElementLine.Fill" Value="{StaticResource 亮蓝}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <VisualState.Setters>
                                        <Setter Target="NormalBorder.Fill" Value="{StaticResource 中黄}" />
                                        <Setter Target="ElementLine.Fill" Value="{StaticResource 亮蓝}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Selected">
                                    <VisualState.Setters>
                                        <Setter Target="SelectedBorder.Visibility" Value="Visible" />
                                    </VisualState.Setters>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Tab PhoneUI模式为缺省样式 -->
    <Style TargetType="a:Tab">
        <Setter Property="Foreground" Value="{StaticResource 默认前景}" />
        <Setter Property="Background" Value="{StaticResource 默认背景}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:Tab">
                    <Grid x:Name="RootGrid" Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <!-- 第一行标题栏 -->
                            <RowDefinition Height="Auto" />
                            <!-- 第二行内容 -->
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Grid
                            x:Name="HeaderGrid"
                            Height="50"
                            Visibility="{Binding HideTitleBar, Converter={StaticResource CvBoolToCollapsed}, RelativeSource={RelativeSource TemplatedParent}}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition MinWidth="10" Width="Auto" />
                                <!-- 标题和菜单平分 -->
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <!-- 后退 -->
                            <Button
                                x:Name="BackButton"
                                Content="&#xE010;"
                                Style="{StaticResource 浅字符按钮}"
                                VerticalAlignment="Stretch"
                                Visibility="{TemplateBinding BackButtonVisibility}"
                                Width="50" />
                            <!-- 标题，不再加窗口标题作为后缀 -->
                            <TextBlock
                                Text="{TemplateBinding Title}"
                                FontSize="{StaticResource 标题字}"
                                Foreground="{StaticResource 默认背景}"
                                Margin="10,0,10,0"
                                TextTrimming="CharacterEllipsis"
                                TextWrapping="NoWrap"
                                VerticalAlignment="Center"
                                Grid.Column="1" />
                            <!-- 触发右键菜单 -->
                            <Border
                                x:Name="ContextMenuBorder"
                                Background="Transparent"
                                Grid.Column="1" />
                            <!-- 菜单 -->
                            <ContentPresenter
                                Content="{TemplateBinding Menu}"
                                Foreground="{StaticResource 默认背景}"
                                HorizontalAlignment="Right"
                                Grid.Column="2" />
                        </Grid>
                        <!-- 内容 -->
                        <!-- 用在win模式的Tabs、phone模式的Tab、Dlg，为Lv Fv等内容提供区域大小 -->
                        <a:SizedPresenter
                            Content="{TemplateBinding Content}"
                            ContentTransitions="{TemplateBinding ContentTransitions}"
                            Grid.Row="1" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- PhoneTabs -->
    <Style TargetType="a:PhoneTabs">
        <Setter Property="Foreground" Value="{StaticResource 默认前景}" />
        <Setter Property="Background" Value="{StaticResource 默认背景}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="a:PhoneTabs">
                    <Grid x:Name="RootGrid" Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="50" />
                        </Grid.RowDefinitions>
                        <!-- 为uno节省一级ContentPresenter！ -->
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- PhoneTabs底部按钮 -->
    <Style x:Key="PhoneTabsBtn" TargetType="Button">
        <Setter Property="Foreground" Value="{StaticResource 默认前景}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid x:Name="RootGrid" Background="Transparent">
                        <StackPanel VerticalAlignment="Center">
                            <TextBlock
                                x:Name="ElementIcon"
                                Text="{Binding Icon, Converter={StaticResource CvIconToChar}}"
                                FontFamily="{StaticResource IconFont}"
                                FontSize="20"
                                HorizontalAlignment="Center"
                                TextAlignment="Center" />
                            <TextBlock
                                x:Name="ElementTitle"
                                Text="{Binding Title}"
                                FontFamily="{TemplateBinding FontFamily}"
                                FontSize="12"
                                HorizontalAlignment="Center"
                                TextWrapping="NoWrap" />
                        </StackPanel>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="PointerOver">
                                    <VisualState.Setters>
                                        <Setter Target="RootGrid.Background" Value="{StaticResource 浅黄}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <VisualState.Setters>
                                        <Setter Target="RootGrid.Background" Value="{StaticResource 中黄}" />
                                    </VisualState.Setters>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <VisualState.Setters>
                                        <Setter Target="ElementIcon.Foreground" Value="{StaticResource 亮蓝}" />
                                        <Setter
                                            Target="ElementTitle.Foreground"
                                            Value="{StaticResource 亮蓝}" />
                                    </VisualState.Setters>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- ToolWindow -->
    <Style TargetType="docking:ToolWindow">
        <Setter Property="Background" Value="{StaticResource 默认背景}" />
        <Setter Property="Foreground" Value="{StaticResource 默认前景}" />
        <Setter Property="MinHeight" Value="200" />
        <Setter Property="MinWidth" Value="400" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="docking:ToolWindow">
                    <Grid x:Name="RootGrid" Background="{TemplateBinding Background}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="40" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Border
                            x:Name="HeaderElement"
                            Background="{StaticResource 中灰1}"
                            BorderBrush="{StaticResource 中灰2}"
                            BorderThickness="0,0,0,1">
                            <StackPanel
                                HorizontalAlignment="Center"
                                Orientation="Horizontal"
                                VerticalAlignment="Center">
                                <Ellipse
                                    Fill="{StaticResource 默认前景}"
                                    Height="6"
                                    Margin="2"
                                    Width="6" />
                                <Ellipse
                                    Fill="{StaticResource 默认前景}"
                                    Height="6"
                                    Margin="2"
                                    Width="6" />
                                <Ellipse
                                    Fill="{StaticResource 默认前景}"
                                    Height="6"
                                    Margin="2"
                                    Width="6" />
                            </StackPanel>
                        </Border>
                        <ContentPresenter Content="{TemplateBinding Content}" Grid.Row="1" />
                        <Rectangle
                            IsHitTestVisible="False"
                            Margin="0,0,-1,0"
                            Stroke="{StaticResource 中灰2}"
                            Grid.RowSpan="2" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Compass -->
    <Style TargetType="docking:Compass">
        <Setter Property="Width" Value="110" />
        <Setter Property="Height" Value="110" />
        <Setter Property="Background" Value="#7FFFFFFF" />
        <Setter Property="BorderBrush" Value="#33000000" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="docking:Compass">
                    <Border
                        x:Name="VisualRoot"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="0"
                        CornerRadius="5">
                        <Grid x:Name="Visual">
                            <Path
                                x:Name="PART_LeftIndicator"
                                Data="M164.833,52.333 L54.167,120.648 L164.833,187.667 z"
                                Fill="{StaticResource 主蓝}"
                                Height="32"
                                HorizontalAlignment="Left"
                                Stretch="Fill"
                                Width="32" />
                            <Path
                                x:Name="PART_TopIndicator"
                                Data="M39.2239,0.5 L84.5,84.5 L0.5,84.5 z"
                                Fill="{StaticResource 主蓝}"
                                Height="32"
                                Stretch="Fill"
                                VerticalAlignment="Top"
                                Width="32" />
                            <Path
                                x:Name="PART_RightIndicator"
                                Data="M69.834,52.833 L69.834,187.166 L182.166,119.618 z"
                                Fill="{StaticResource 主蓝}"
                                Height="32"
                                HorizontalAlignment="Right"
                                Stretch="Fill"
                                Width="32" />
                            <Path
                                x:Name="PART_BottomIndicator"
                                Data="M272.667,456.333 L327.333,456.667 L300,505.664 z"
                                Fill="{StaticResource 主蓝}"
                                Height="32"
                                Stretch="Fill"
                                VerticalAlignment="Bottom"
                                Width="32" />
                            <Rectangle
                                x:Name="PART_CenterIndicator"
                                Fill="White"
                                Height="35"
                                Stroke="{StaticResource 主蓝}"
                                Width="35" />
                        </Grid>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="HighlightLeftIndicator">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_LeftIndicator"
                                            Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource 亮红}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="HighlightTopIndicator">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_TopIndicator"
                                            Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource 亮红}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="HighlightRightIndicator">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_RightIndicator"
                                            Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource 亮红}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="HighlightBottomIndicator">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_BottomIndicator"
                                            Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource 亮红}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="HighlightCenterIndicator">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_CenterIndicator"
                                            Storyboard.TargetProperty="Fill">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource 亮红}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="LeftIndicatorVisibilityStates">
                                <VisualState x:Name="LeftIndicatorVisibile" />
                                <VisualState x:Name="LeftIndicatorHidden">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_LeftIndicator"
                                            Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="TopIndicatorVisibilityStates">
                                <VisualState x:Name="TopIndicatorVisibile" />
                                <VisualState x:Name="TopIndicatorHidden">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_TopIndicator"
                                            Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="RightIndicatorVisibilityStates">
                                <VisualState x:Name="RightIndicatorVisibile" />
                                <VisualState x:Name="RightIndicatorHidden">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_RightIndicator"
                                            Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="BottomIndicatorVisibilityStates">
                                <VisualState x:Name="BottomIndicatorVisibile" />
                                <VisualState x:Name="BottomIndicatorHidden">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_BottomIndicator"
                                            Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

                            <VisualStateGroup x:Name="CenterIndicatorVisibilityStates">
                                <VisualState x:Name="CenterIndicatorVisibile" />
                                <VisualState x:Name="CenterIndicatorHidden">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames
                                            Storyboard.TargetName="PART_CenterIndicator"
                                            Storyboard.TargetProperty="Visibility">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- RootCompass -->
