<a:Win
    x:Class="Dt.Base.FvDesign"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

    <a:Tab x:Name="_tabMain" Title="表单">
        <a:Tab.Menu>
            <a:Menu>
                <a:Mi ID="增加" Icon="加号" Call="OnAdd" BtnCall="OnBatchAdd" />
                <a:Mi ID="删除" Icon="删除" Call="OnDel" />
                <a:Mi ID="Xaml" Icon="HtmL" Call="OnShowEditXaml" ShowBtn="True" BtnCall="OnCopyXaml" />
                <a:Mi ID="确定" Icon="保存" Call="OnSave" />
            </a:Menu>
        </a:Tab.Menu>
    </a:Tab>
    <a:Tab Title="属性" a:Ex.Dock="Right">
        <a:Fv x:Name="_fvProp" />
    </a:Tab>
</a:Win>
