<a:Win
    x:Class="Dt.Base.Tools.TypeAliasWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

    <a:Pane>
        <a:Tabs>
            <a:Tab Title="别名标签" Order="1">
                <a:Lv x:Name="_lvAttr" ItemClick="OnAttrItemClick">
                    <DataTemplate>
                        <StackPanel Margin="10">
                            <a:Dot ID="name" />
                            <a:Dot ID="count" Call="Def.小灰" />
                        </StackPanel>
                    </DataTemplate>
                </a:Lv>
            </a:Tab>
            <a:Tab Title="多类型别名标签" Order="2">
                <a:Lv x:Name="_lvGroup" ItemClick="OnGroupItemClick">
                    <DataTemplate>
                        <StackPanel Margin="10">
                            <a:Dot ID="name" />
                            <a:Dot ID="count" Call="Def.小灰" />
                        </StackPanel>
                    </DataTemplate>
                </a:Lv>
            </a:Tab>
        </a:Tabs>
    </a:Pane>


    <a:Tab Title="类型列表">
        <a:Lv x:Name="_lvType">
            <DataTemplate>
                <StackPanel Margin="10">
                    <a:Dot ID="alias" />
                    <a:Dot ID="types" Call="Def.小灰" />
                </StackPanel>
            </DataTemplate>
        </a:Lv>
    </a:Tab>
</a:Win>
