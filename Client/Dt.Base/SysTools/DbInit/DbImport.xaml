<a:Tab
    x:Class="Dt.Base.Tools.DbImport"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="导入数据">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Button x:Name="_btnDt" Content="导入初始表结构及数据" Click="OnDt" Background="Green" Style="{StaticResource 浅色按钮}" HorizontalAlignment="Stretch" />
            <Button x:Name="_btnOther" Content="导　入　其　它" Click="OnOther" Background="{StaticResource 主蓝}" Style="{StaticResource 浅色按钮}" HorizontalAlignment="Stretch" Grid.Column="1" />
        </Grid>
        <TextBox x:Name="_tbInfo"
                 AcceptsReturn="True"
                 TextWrapping="Wrap"
                 ScrollViewer.VerticalScrollMode="Enabled"
                 ScrollViewer.VerticalScrollBarVisibility="Auto"
                 Grid.Row="1" />
    </Grid>
</a:Tab>