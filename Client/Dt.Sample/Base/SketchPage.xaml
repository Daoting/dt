<a:Win
    x:Class="Dt.Sample.SketchPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">
    <StackPanel Orientation="Vertical" >
        <StackPanel x:Name="_sp" Orientation="Vertical">
            <Button x:Name="_import" Content="导入" />
            <a:Sketch x:Name="_sketch" IsReadOnly="True" 
                       ShowHorRuler="True"
                       ShowVerRuler="True"
                       ShowGridLine="True" />
        </StackPanel>
    </StackPanel>
</a:Win>
