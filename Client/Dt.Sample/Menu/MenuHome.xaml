<a:Win
    x:Class="Dt.Sample.MenuHome"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Home="功能列表">

    <a:WinItem DockState="DockedLeft">
        <a:Tabs>
            <a:Tab Title="功能列表">
                <a:Lv x:Name="_lv" Style="{StaticResource CenterInfoLv}" ItemClick="OnCenterInfoClick" />
            </a:Tab>
        </a:Tabs>
    </a:WinItem>
</a:Win>
