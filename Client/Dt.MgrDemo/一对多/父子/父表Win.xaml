<a:Win
    x:Class="Dt.MgrDemo.一对多.父表Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.MgrDemo.一对多">

    <l:父表List x:Name="_parentList" Order="1" a:Ex.Dock="Left" />
    <l:父表Form x:Name="_parentForm" />
    <a:Tabs a:Ex.Dock="Bottom">
        <l:父表大儿List x:Name="_大儿List" />
        <l:父表小儿List x:Name="_小儿List" />
    </a:Tabs>
    <a:Tab x:Name="_childForm" Title="表单" a:Ex.Dock="Right" />
</a:Win>
