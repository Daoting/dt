<a:Win
    x:Class="Dt.MgrDemo.Hook.HookWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.MgrDemo.Hook">

    <a:Pane>
        <a:Tabs>
            <a:Tab Order="1">
                <l:HookList x:Name="_list" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab>
                <l:HookForm x:Name="_form" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
</a:Win>
