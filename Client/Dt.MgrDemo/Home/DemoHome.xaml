<a:Win
    x:Class="Dt.MgrDemo.DemoHome"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:home="using:Dt.Mgr.Home"
    xmlns:l="using:Dt.MgrDemo"
    AutoSaveLayout="False">

    <a:Pane>
        <a:Tabs>
            <a:Tab Order="2" Icon="主页">
                <home:RootMenu x:Name="_menu" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab Order="1" Icon="搬运工">
                <l:DemoMain x:Name="_demo" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
    <a:Pane Pos="Right">
        <a:Tabs>
            <a:Tab Order="3">
                <home:MyMain x:Name="_my" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
