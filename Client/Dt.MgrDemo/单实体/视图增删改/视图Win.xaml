<a:Win
    x:Class="Dt.MgrDemo.视图Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.MgrDemo">

    <a:Tabs a:Ex.Dock="Left">
        <a:FuzzySearch x:Name="_search" Search="OnSearch">
            <x:String>全部</x:String>
        </a:FuzzySearch>
        <l:视图Query x:Name="_query" Query="OnQuery" />
    </a:Tabs>
    <l:视图List x:Name="_list" Order="1" />
    <l:视图Form x:Name="_form" a:Ex.Dock="Right" />
</a:Win>
