<a:Win
    x:Class="Dt.MgrDemo.单实体.实体Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.MgrDemo.单实体">

    <a:Pane>
        <a:Tabs>
            <a:Tab Icon="搜索">
                <a:SearchMv x:Name="_search" Search="OnSearch">
                    <x:String>全部</x:String>
                </a:SearchMv>
            </a:Tab>
            <a:Tab Icon="漏斗">
                <l:实体Query x:Name="_query" Query="OnQuery" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab Order="1">
                <l:实体List x:Name="_list" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
    <a:Pane Pos="Right">
        <a:Tabs>
            <a:Tab>
                <l:实体Form x:Name="_form" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
