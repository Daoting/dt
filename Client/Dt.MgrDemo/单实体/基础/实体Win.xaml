<a:Win
    x:Class="Dt.MgrDemo.单实体.实体Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.MgrDemo.单实体">

    <a:Tabs a:Ex.Dock="Left">
        <a:FuzzySearch x:Name="_search" Search="OnSearch" Icon="搜索">
            <x:String>全部</x:String>
        </a:FuzzySearch>
        <l:实体Query x:Name="_query" Query="OnQuery" Icon="漏斗" />
    </a:Tabs>
    <l:实体List x:Name="_list" Order="1" />
    <l:实体Form x:Name="_form" a:Ex.Dock="Right" />
</a:Win>
