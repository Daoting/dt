<a:Form
    x:Class="Dt.Mgr.Module.RptViewParamsForm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="报表参数">

    <a:Dlg.Menu>
        <a:Menu>
            <a:Mi ID="增加" Icon="加号" Call="Add" />
            <a:Mi ID="删除" Icon="删除" Call="Delete" />
        </a:Menu>
    </a:Dlg.Menu>

    <a:Fv x:Name="_fv" Save="Save" Create="Add" Delete="Delete">
        <a:CText ID="name" Title="参数名" />
        <a:CText ID="val" Title="值" />
    </a:Fv>
</a:Form>
