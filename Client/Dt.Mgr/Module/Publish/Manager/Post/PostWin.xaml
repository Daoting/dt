<a:Win
    x:Class="Dt.Mgr.Module.PostWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Module">

    <l:PostxList x:Name="_list" Order="1" a:Ex.Dock="Left" />
    <l:PostForm x:Name="_form" />
    <a:Tabs a:Ex.Dock="Right">
        <l:PostKeywordList x:Name="_keywordList" />
        <l:PostAlbumList x:Name="_albumList" />
    </a:Tabs>
</a:Win>
