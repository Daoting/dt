<a:Win
    x:Class="Dt.Mgr.Module.OptionGroupWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Module">

    <a:Pane>
        <a:Tabs InitHeight="600">
            <l:OptionGroupList x:Name="_parentList" Order="1" />
        </a:Tabs>
        <a:Tabs InitHeight="400">
            <l:OptionGroupForm x:Name="_parentForm" />
        </a:Tabs>
    </a:Pane>
    <l:OptionGroupOptionList x:Name="_optionList" />
    <a:Tab x:Name="_childForm" Title="表单" a:Ex.Dock="Right" />
</a:Win>
