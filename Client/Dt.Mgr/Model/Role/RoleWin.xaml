<a:Win
    x:Class="Dt.Mgr.Model.RoleWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Model">

    <l:RoleList x:Name="_list" Order="1" a:Ex.Dock="Left" />
    <l:RoleForm x:Name="_form" />
    <a:Tabs a:Ex.Dock="Right">
        <l:RoleUserList x:Name="_userList" />
        <l:RoleMenuList x:Name="_menuList" />
        <l:RolePrvList x:Name="_prvList" />
    </a:Tabs>
</a:Win>
