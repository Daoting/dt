<a:Win
    x:Class="Dt.Mgr.Model.PrvWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Model">

    <l:PrvList x:Name="_list" Order="1" a:Ex.Dock="Left" />
    <l:PrvForm x:Name="_form" />
    <a:Tabs a:Ex.Dock="Right">
        <l:PrvRoleList x:Name="_roleList" />
        <l:PrvUserList x:Name="_userList" />
    </a:Tabs>
</a:Win>
