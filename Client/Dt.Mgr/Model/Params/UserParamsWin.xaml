<a:Win
    x:Class="Dt.Mgr.Model.UserParamsWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Model">

    <a:FuzzySearch x:Name="_search" Placeholder="参数名" Search="OnSearch" a:Ex.Dock="Left">
        <x:String>全部</x:String>
        <x:String>最近修改</x:String>
    </a:FuzzySearch>
    <l:UserParamsList x:Name="_list" Order="1" />
    <l:UserParamsForm x:Name="_form" a:Ex.Dock="Right" />
</a:Win>
