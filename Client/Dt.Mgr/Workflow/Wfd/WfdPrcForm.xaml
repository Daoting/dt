<a:Form
    x:Class="Dt.Mgr.Workflow.WfdPrcForm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="流程模板">

    <a:Fv x:Name="_fv" Create="Add" Delete="Delete" Save="Save">
        <a:CText ID="name" Title="流程名称" />
        <a:CText ID="note" Title="描述" />
        <a:CTip ID="ctime" Title="创建时间" />
        <a:CTip ID="mtime" Title="修改时间" />
        <a:CBool ID="is_locked" Title="锁定不可创建实例" ShowTitle="False" />
        <a:CBool ID="singleton" Title="同一时刻只可单例" ShowTitle="False" />
    </a:Fv>
</a:Form>