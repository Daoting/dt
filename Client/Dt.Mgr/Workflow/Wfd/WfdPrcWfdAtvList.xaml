<a:List
    x:Class="Dt.Mgr.Workflow.WfdPrcWfdAtvList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="活动列表">
    
    <a:Lv x:Name="_lv">
        <a:Cols>
            <a:Col ID="name" Title="活动名称" />
            <a:Col ID="type" Title="活动类别" />
            <a:Col ID="exec_scope" Title="执行者范围" />
            <a:Col ID="exec_limit" Title="执行者限制" />
            <a:Col ID="auto_accept" Title="自动签收" />
            <a:Col ID="can_delete" Title="可删除流程实例" Width="140" />
            <a:Col ID="can_terminate" Title="可中止流程实例" Width="140" />
            <a:Col ID="can_jump_into" Title="可为跳转目标" Width="140" />
            <a:Col ID="trans_kind" Title="后续迁移方式" Width="140" />
            <a:Col ID="join_kind" Title="聚合方式" />
            <a:Col ID="ctime" Title="创建时间" />
            <a:Col ID="mtime" Title="修改时间" />
        </a:Cols>
    </a:Lv>
</a:List>
