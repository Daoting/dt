<a:Win
    x:Class="Dt.Mgr.Workflow.WfdPrcWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Workflow">

    <l:WfdPrcQuery x:Name="_query" Order="1" a:Ex.Dock="Left" />
    <l:WfdPrcList x:Name="_parentList" />
    <a:Tabs a:Ex.Dock="Bottom">
        <l:WfdPrcWfdAtvList x:Name="_wfdAtvList" />
        <l:WfdPrcWfdTrsList x:Name="_wfdTrsList" />
    </a:Tabs>
</a:Win>
