<a:Win
    x:Class="Dt.Mgr.Rbac.GroupWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Rbac">

    <a:FuzzySearch x:Name="_query" Search="OnSearch" a:Ex.Dock="Left" Order="1">
        <x:String>全部</x:String>
    </a:FuzzySearch>
    <l:GroupList x:Name="_mainList" />
    <a:Tabs a:Ex.Dock="Right">
        <l:GroupUserList x:Name="_userList" />
        <l:GroupRoleList x:Name="_roleList" />
    </a:Tabs>
</a:Win>
