<a:Win
    x:Class="Dt.Mgr.Rbac.GroupWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Rbac">

    <l:GroupList x:Name="_mainList" Order="1" a:Ex.Dock="Left" />
    <l:GroupForm x:Name="_mainForm" />
    <a:Tabs a:Ex.Dock="Right">
        <l:GroupRoleList x:Name="_roleList" />
        <l:GroupUserList x:Name="_userList" />
    </a:Tabs>
</a:Win>
