<a:Win
    x:Class="Dt.Mgr.Rbac.PerWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Rbac">

    <l:PerModuleList x:Name="_mouleList" Order="1" a:Ex.Dock="Left" />
    <a:Main>
        <a:Tabs InitHeight="300">
            <l:PerFuncList x:Name="_funcList" />
        </a:Tabs>
        <a:Tabs InitHeight="300">
            <l:PerList x:Name="_perList" />
        </a:Tabs>
    </a:Main>
    <l:PerRoleList x:Name="_perRoleList" a:Ex.Dock="Right" />
</a:Win>
