<a:Win
    x:Class="Dt.Mgr.Rbac.RoleWin"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.Mgr.Rbac">

    <a:FuzzySearch x:Name="_query" a:Ex.Dock="Left" Order="1">
        <x:String>全部</x:String>
    </a:FuzzySearch>
    <l:RoleList x:Name="_mainList" />
    <a:Tabs a:Ex.Dock="Right">
        <l:RoleUserList x:Name="_userList" />
        <l:RoleGroupList x:Name="_groupList" />
        <l:RoleMenuList x:Name="_menuList" />
        <l:RolePerList x:Name="_permissionList" />
    </a:Tabs>
</a:Win>
