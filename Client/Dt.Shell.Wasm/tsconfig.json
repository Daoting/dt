{
  "compilerOptions": {
    "module": "None",
    "declaration": false,
    "diagnostics": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "noImplicitThis": true,
    "target": "ES2017",
    "outFile": "WasmScripts/webrtc.js"
  },
  "include": [
    "TypeScript/**/*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
