<a:Win
    x:Class="Dt.App.Workflow.WorkflowMgr"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Home="功能目录">

    <a:WinItem DockState="DockedLeft" InitWidth="240">
        <a:Tabs>
            <a:Tab Title="功能目录">
                <a:Lv x:Name="_lv" Style="{StaticResource CenterInfoLv}" ItemClick="OnCenterInfoClick" />
            </a:Tab>
        </a:Tabs>
    </a:WinItem>
</a:Win>
