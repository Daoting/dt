<a:Win
    x:Class="Dt.App.DefaultHome"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:home="using:Dt.App.Home"
    AutoSaveLayout="False"
    Home="开始,动态,我的">

    <a:Pane>
        <a:Tabs>
            <a:Tab Title="开始">
                <home:RootMenu x:Name="_menu" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab Title="动态">
                
            </a:Tab>
        </a:Tabs>
    </a:Main>
    <a:Pane Pos="Right">
        <a:Tabs>
            <a:Tab Title="我的">
                <home:MyMain x:Name="_my" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
</a:Win>
