<a:Win
    x:Class="Dt.App.Home.DefaultHome"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:home="using:Dt.App.Home"
    AutoSaveLayout="False"
    Home="开始,动态,设置">

    <a:WinItem DockState="DockedLeft">
        <a:Tabs>
            <a:Tab Title="开始">
                <home:RootMenu x:Name="_menu" />
            </a:Tab>
        </a:Tabs>
    </a:WinItem>
    <a:WinCenter>
        <a:Tabs>
            <a:Tab Title="动态">
                
            </a:Tab>
        </a:Tabs>
    </a:WinCenter>
    <a:WinItem DockState="DockedRight">
        <a:Tabs>
            <a:Tab Title="设置">
                
            </a:Tab>
        </a:Tabs>
    </a:WinItem>
</a:Win>
