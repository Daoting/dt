<a:Win
    x:Class="Dt.App.Model.UserParamsMgr"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Dt.App.Model">

    <a:Pane>
        <a:Tabs>
            <a:Tab Title="查找">
                <a:SearchFv Placeholder="参数名" Search="OnSearch">
                    <a:CBar Title="常用" />
                    <Button Content="全部" />
                    <Button Content="最近修改" />
                </a:SearchFv>
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab Order="1">
                <l:UserParamsList x:Name="_ls" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
</a:Win>
