<a:PageWin
    x:Class="Dt.Kehu.PlanTest4"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="康养计划">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Image Source="ms-appx:///Bs.Kehu/Assets/test3.png" VerticalAlignment="Top" />
        <StackPanel Margin="10" Grid.Row="1">
            <TextBlock Text="运动计划" />
            <TextBlock Text="    平躺向回勾脚" />
            <TextBlock Text="膳食计划" />
            <TextBlock Text="    补充蛋白质" />
        </StackPanel>
        <Grid Grid.Row="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Button Content="返回首页" Click="OnNo" Background="{StaticResource 主题蓝色}" Foreground="White" HorizontalAlignment="Stretch" />
            <Button Content="去执行" Click="OnYes" Background="{StaticResource 醒目蓝色}" Foreground="White" HorizontalAlignment="Stretch" Grid.Column="1" />
        </Grid>
    </Grid>
</a:PageWin>
