<Window
    x:Class="Dt.Shell.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Padding="10">
            <Button Content="图表" Click="OnTest1" />
            <Button Content="月报xls" Tag="MonthProfit.xlsx" Click="OnLoadFile" />
            <Button Content="月报xml" Tag="MonthProfit.ssxml" Click="OnLoadFile" />
        </StackPanel>
        <a:Excel x:Name="_excel" Grid.Row="1" />
    </Grid>
</Window>
