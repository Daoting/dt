		<ImplicitUsings>enable</ImplicitUsings>
		<RootNamespace>$ext_safeprojectname$</RootNamespace>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<NoWarn>1701;1702;1705;1591</NoWarn>
