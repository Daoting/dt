<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0-android</TargetFramework>
		<SingleProject>true</SingleProject>
		<OutputType>Exe</OutputType>
		<IsUnoHead>true</IsUnoHead>
	</PropertyGroup>

	<Import Project="..\$ext_safeprojectname$.Client\Common.props" />

	<ItemGroup>
		<ProjectReference Include="..\$ext_safeprojectname$.Client\$ext_safeprojectname$.Client.csproj" />
	</ItemGroup>

	<ItemGroup>
		<AndroidAsset Include="..\$ext_safeprojectname$.Win\Assets\Fonts\icon.ttf" Link="Android\Assets\Fonts\icon.ttf" />
		<AndroidAsset Include="..\$ext_safeprojectname$.Win\Assets\Fonts\uno-fluentui-assets.ttf" Link="Android\Assets\Fonts\uno-fluentui-assets.ttf" />
	</ItemGroup>
	
	<ItemGroup>
		<Page Include="..\$ext_safeprojectname$.Win\App.xaml" Link="App.xaml" />
		<Compile Include="..\$ext_safeprojectname$.Win\App.xaml.cs" Link="App.xaml.cs">
			<DependentUpon>App.xaml</DependentUpon>
		</Compile>
	</ItemGroup>
</Project>