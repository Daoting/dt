<a:Win
    x:Class="$rootnamespace$.$maincls$Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:$rootnamespace$">

    <a:Pane>
        <a:Tabs>
            <a:Tab Order="1">
                <l:$maincls$List x:Name="_list" />
            </a:Tab>
        </a:Tabs>
    </a:Pane>
    <a:Main>
        <a:Tabs>
            <a:Tab>
                <l:$maincls$Form x:Name="_form" />
            </a:Tab>
        </a:Tabs>
    </a:Main>
    <a:Pane Pos="Right">
        <a:Tabs>
$relatedlist$
        </a:Tabs>
    </a:Pane>
</a:Win>
