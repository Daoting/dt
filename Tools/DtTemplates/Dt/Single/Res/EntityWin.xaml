<a:Win
    x:Class="$rootnamespace$.$clsroot$Win"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:$rootnamespace$">

    <a:Tabs a:Ex.Dock="Left">
        <a:FuzzySearch x:Name="_search" Search="OnSearch">
            <x:String>全部</x:String>
        </a:FuzzySearch>
        <l:$clsroot$Query x:Name="_query" Query="OnQuery" Icon="漏斗" />
    </a:Tabs>
    <l:$clsroot$List x:Name="_list" Order="1" />
    <l:$clsroot$Form x:Name="_form" a:Ex.Dock="Right" />
</a:Win>
