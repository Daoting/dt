<a:RptQuery
    x:Class="Dt.MgrDemo.CustomRptQuery"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base">

	<a:QueryFv x:Name="_fv" AllowFuzzySearch="False">
		<a:CText ID="普通文本" />
		<a:CBool ID="选择" />
		<a:CNum ID="数值" />
		<a:CDate ID="今天" />
		<a:CList ID="下拉选择">
			<a:CList.Items>
				<x:String>选项一</x:String>
				<x:String>选项二</x:String>
				<x:String>选项三</x:String>
				<x:String>选项四</x:String>
				<x:String>选项五</x:String>
				<x:String>选项六</x:String>
			</a:CList.Items>
		</a:CList>
	</a:QueryFv>
</a:RptQuery>
