<a:List
    x:Class="Demo.Lob.供应商List"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="列表">

    <a:Lv x:Name="_lv">
        <a:Cols>
            <a:Col ID="名称" />
            <a:Col ID="执照号" />
            <a:Col ID="执照效期" />
            <a:Col ID="税务登记号" />
            <a:Col ID="地址" />
            <a:Col ID="电话" />
            <a:Col ID="开户银行" />
            <a:Col ID="帐号" />
            <a:Col ID="联系人" />
            <a:Col ID="建档时间" />
            <a:Col ID="撤档时间" />
            <a:Col ID="备注" />
        </a:Cols>
    </a:Lv>
</a:List>
