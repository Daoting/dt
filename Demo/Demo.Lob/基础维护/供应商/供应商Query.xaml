<a:Tab
    x:Class="Demo.Lob.供应商Query"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="查询"
    Icon="漏斗">

    <a:QueryFv x:Name="_fv">
        <a:CText ID="名称" Query="Editable" QueryFlag="Contains" />
        <a:CText ID="电话" Query="Editable" QueryFlag="Equal" />
        <a:CText ID="帐号" Query="Editable" QueryFlag="Equal" />
        <a:CText ID="联系人" Query="Editable" QueryFlag="Equal" />
    </a:QueryFv>
</a:Tab>