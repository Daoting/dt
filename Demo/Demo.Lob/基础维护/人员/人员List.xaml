<a:List
    x:Class="Demo.Lob.人员List"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    Title="人员列表">

    <a:Lv x:Name="_lv">
        <a:Cols>
            <a:Col ID="姓名" />
            <a:Col ID="账号" />
            <a:Col ID="出生日期" />
            <a:Col ID="性别" />
            <a:Col ID="工作日期" />
            <a:Col ID="办公室电话" />
            <a:Col ID="电子邮件" />
            <a:Col ID="建档时间" />
            <a:Col ID="撤档时间" />
            <a:Col ID="撤档原因" />
        </a:Cols>
    </a:Lv>
</a:List>
