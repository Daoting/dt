<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);1701;1702;1705;1591;IL2026;IL2046;IL2055;IL2057;IL2060;IL2065;IL2067;IL2070;IL2072;IL2075;IL2077;IL2080;IL2087;IL2090;IL2091;IL2093;IL2111;IL2112;IL2122;IL3000;IL3002;IL3050</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Dt.Service" Version="5.2.0" />
	</ItemGroup>

	<!-- 确保使用github源码调试dt包 -->
	<PropertyGroup Condition="'$(Configuration)' == 'Debug'">
		<!-- 将包中的pdb文件复制到输出目录，默认不复制 -->
		<CopyDebugSymbolFilesFromPackages>true</CopyDebugSymbolFilesFromPackages>
	</PropertyGroup>

	<ItemGroup>
		<RdXmlFile Include="rd.xml" />
	</ItemGroup>
	
	<ItemGroup>
		<Using Include="Dt.Core" />
		<Using Include="Serilog" />
		<Using Include="System.Linq" />
		<Using Include="System.Threading.Tasks" />
	</ItemGroup>
</Project>
