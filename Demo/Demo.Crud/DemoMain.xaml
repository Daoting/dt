<a:Win
    x:Class="Demo.Crud.DemoMain"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="using:Dt.Base"
    xmlns:l="using:Demo.Crud"
    Title="管理样例">

    <a:NavList
        x:Name="_navBase"
        Title="基础"
        Icon="搬运工"
        Order="1"
        To="NewWin"
        a:Ex.Dock="Left" />
    <a:NavList
        x:Name="_navEntity"
        Title="实体"
        Icon="词典"
        Order="2"
        To="NewWin"
        ViewMode="Tile" />
    <l:RptHome Order="3" a:Ex.Dock="Right" />
</a:Win>
